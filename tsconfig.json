{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "lib": [
      "es2017"
    ],
    "locale": "en-us",
    "module": "commonjs",
    "moduleResolution": "node",
    "newLine": "lf",
    "strict": true,
    "strictNullChecks": true,
    "target": "es2017",
    "typeRoots": [
      "node_modules/@types",
      "src/custom-typings"
    ]
  }
}
